{
  "name": "Asset_Mgt",
  "version": "1.0.0",
  "description": "Asset",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix asset",
    "start": "node server/server.js",
    "server": "nodemon server/server.js --exec babel-node --presets babel-preset-env",
    "assets": "npm start --prefix asset",
    "dev": "concurrently \"npm run server\" \"npm run assets\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix asset && npm run build --prefix asset"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^3.6.0",
    "config": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.1.6",
    "socket.io": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.17.5"
  }
}
